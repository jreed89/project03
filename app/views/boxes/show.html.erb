<!-- lines 7 and 8 should show the total weight and value -->

<h2> Box: <%= @boxes.label %></h2>
<%= link_to "Edit Box", box_edit_path(:box_id => @boxes.id)%>
<%= link_to "New Item", items_new_path(:box_id => @boxes.id), { :class => "button waves-effect waves-light btn" } %>
  <br><br>
  <div class="row">
    <% @items.each do |item| %>
        <h1><%= item.name %></h1>
		<h4><%= item.description %></h4>
		<h4><%= item.weight %>: lbs</h4>
		<h4>$<%= item.value %> Dollars</h4>
    <% end %>
    <h1>Total <%# item.total %></h1>
  </div>
  