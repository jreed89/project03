<%= form_for @boxes do |box| %>
   <div class="container #e0f7fa cyan lighten-5">
    <%= box.label :label, 'Name'%>
    <%= box.text_field :label%>
  </div>
   <div class="container #b2ebf2 cyan lighten-4">
    <%= box.label :length, 'Length' %>
    <%= box.text_field :length %>
  </div>
   <div class="container #80deea cyan lighten-3">
    <%= box.label :width, 'Width' %>
    <%= box.text_field :width %>
  </div>
   <div class="container #4dd0e1 cyan lighten-2">
    <%= box.label :height, 'Height' %>
    <%= box.text_field :height %>
  </div>
   <div class="container #26c6da cyan lighten-1">
      <%= box.label :sealed, 'Sealed' %>
      <%= box.text_field :sealed %>
  </div>
   <div class="container #00bcd4 cyan">
    <%= box.label :arrived, 'Arrived' %>
    <%= box.text_field :arrived %>
  </div>
 <div class="center"><button class="#00b8d4 cyan accent-4"><%= box.submit "Submit" %></button></div>
<% end %>
